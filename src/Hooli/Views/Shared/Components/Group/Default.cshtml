@{
    // ViewBag.Title = "Home Page";
}

@model List<Hooli.Models.Group>

@if (Model != null)
{
    @foreach (var p in Model)
    {
        <div class="info-photo-container col-sm-12 col-md-6">
            <div class="info-photo" style="background: url() center / cover;">
                <div class="info-photo-shade">
                    <a asp-controller="Group" asp-action="SingleGroup"><h1>@p.GroupName</h1></a>
                </div>
            </div>
        </div>

    }
}